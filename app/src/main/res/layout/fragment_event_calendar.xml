<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="wasteit.wasteit.EventCalendarFragment">
    <Spinner
        android:layout_width="wrap_content" android:id="@+id/calendar_currency"
        android:layout_height="wrap_content"></Spinner>
    <customcalendar.CalendarView
        xmlns:calendarNS="http://schemas.android.com/apk/res"
        android:id="@+id/calendar_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        calendarNS:dateFormat="MMMM yyyy"/>
</FrameLayout>
